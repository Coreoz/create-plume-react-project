  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all",
    "Firefox >= 75",
    "Chrome >= 80",
    "Firefox >= 75",
    "safari >= 12"
  ],
  "scripts": {
    "start": "concurrently \"yarn run watch-ts\" \"yarn run watch-scss-module\" \"yarn run watch-js\"",
    "watch-ts": "ttsc --project tsconfig.json -w",
    "watch-js": "vite",
    "build": "rm -rf build && ttsc && vite build",
    "stylelint": "stylelint '**/*.scss'",
    "eslint": "eslint src --ext .ts,.tsx",
    "typescript": "tsc --project tsconfig.json --noEmit",
    "test": "jest --config jest.config.js",
    "coverage": "jest --config jest.config.js --coverage --watchAll=false",
    "watch-scss-module": "typed-scss-modules 'src/**/*.scss' -w"
  },
  "packageManager": "yarn@3.1.1"
}
