    "typed-scss-modules": "^7.1.1",
    "typescript": "4.9.4",
    "vite": "^5.1.4"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all",
    "Firefox >= 75",
    "Chrome >= 80",
    "Firefox >= 75",
    "safari >= 12"
  ],
  "scripts": {
    "start": "concurrently  \"yarn watch-ts\" \"yarn copy-scss\" \"yarn watch-scss-module\" \"yarn watch-js\"",
    "watch-ts": "ttsc --incremental -w",
    "watch-js": "vite",
    "watch-scss-module": "typed-scss-modules 'src/**/*.scss' -w -u",
    "copy-scss": "copyfiles -u 1 ./src/**/*.scss ts-built",
    "build": "rm -rf build && yarn copy-scss && ttsc && vite build",
    "stylelint": "stylelint '**/*.scss'",
    "eslint": "eslint src --ext .ts,.tsx -c .eslintrc.ci.js",
    "typescript": "tsc --noEmit",
    "test": "jest --config jest.config.ts",
    "coverage": "jest --config jest.config.ts --coverage --watchAll=false",
    "check-scss-types": "typed-scss-modules 'src/**/*.scss' -l",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "packageManager": "yarn@3.6.4",
  "engines": {
    "node": "~18"
  }
}
