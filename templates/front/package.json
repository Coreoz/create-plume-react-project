    "plume-ts-di": "^1.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.37.0",
    "react-router-dom": "^6.4.2",
    "simple-http-request-builder": "^2.0.0",
    "simple-http-rest-client": "^2.0.2",
    "simple-job-scheduler": "^1.1.2",
    "simple-logging-system": "^1.1.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.1.2",
    "@types/node": "^20.3.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "concurrently": "^7.4.0",
    "copyfiles": "^2.4.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-typescript-compat": "^0.3.1",
    "fetch-mock": "^9.11.0",
    "fetch-mock-jest": "^1.5.1",
    "history": "5.3.0",
    "jest": "^29.1.2",
    "jest-config": "^29.1.2",
    "jsdom": "^20.0.1",
    "node-fetch": "^2.6.7",
    "postcss": "^8.4.18",
    "react-router": "^6.4.2",
    "sass": "^1.55.0",
    "stylelint": "^14.13.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "ts-transformer-classname": "^2.0.0",
    "tslib": "^2.5.3",
    "ttypescript": "^1.5.15",
    "typed-scss-modules": "^7.1.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-watch-and-run": "^1.1.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all",
    "Firefox >= 75",
    "Chrome >= 80",
    "Firefox >= 75",
    "safari >= 12"
  ],
  "scripts": {
    "start": "concurrently  \"yarn watch-ts\" \"yarn copy-css\" \"yarn watch-scss-module\" \"yarn watch-js\"",
    "watch-ts": "ttsc --incremental -w",
    "watch-js": "vite",
    "watch-scss-module": "typed-scss-modules 'src/**/*.scss' -w -u",
    "copy-css": "copyfiles -u 1 ./src/**/*.scss ts-built",
    "build": "rm -rf build && ttsc && vite build",
    "stylelint": "stylelint '**/*.scss'",
    "eslint": "eslint src --ext .ts,.tsx",
    "typescript": "tsc --noEmit",
    "test": "jest --config jest.config.js",
    "coverage": "jest --config jest.config.js --coverage --watchAll=false",
    "check-scss-types": "typed-scss-modules 'src/**/*.scss' -l"
  },
  "packageManager": "yarn@3.3.1"
}
